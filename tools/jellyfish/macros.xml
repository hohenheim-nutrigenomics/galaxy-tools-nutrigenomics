<?xml version="1.0"?>
<macros>
  <xml name="requirements">
    <requirements>
      <requirement type="package" version="2.2.3">jellyfish</requirement>
      <yield/>
    </requirements>
  </xml>
  <token name="@WRAPPER_VERSION@">2.2.3</token>
  <xml name="stdio">
    <stdio>
      <!-- Anything other than zero is an error -->
      <exit_code range="1:"/>
      <exit_code range=":-1"/>
      <!-- In case the return code has not been set propery check stderr too -->
      <regex match="Error:"/>
      <regex match="Exception:"/>
    </stdio>
  </xml>
  <token name="@ATTRIBUTION@"><![CDATA[
**Attribution**

This Galaxy tool relies on jellyfish from  `UMD Genome Assembly Group
<http://www.genome.umd.edu/jellyfish.html>`_, developed by Guillaume Marcais and Carl Kingsford
  ]]>
  </token>
  <xml name="citation">
      <citations>
      </citations>
  </xml>
</macros>


